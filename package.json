    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-pkg": "^0.21.2",
    "prettier-plugin-sh": "^0.18.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "serve": "^14.2.5",
    "tsdown": "^0.17.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  }
